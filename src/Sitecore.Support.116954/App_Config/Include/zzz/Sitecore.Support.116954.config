<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <databases>
      <database id="master">
        <workflowProvider hint="defer" type="Sitecore.Support.Workflows.Simple.WorkflowProvider, Sitecore.Support.116954" patch:instead="*[@type='Sitecore.Workflows.Simple.WorkflowProvider, Sitecore.Kernel']">
          <param desc="database">$(id)</param>
          <param desc="history store" ref="workflowHistoryStores/main" param1="$(id)" />
        </workflowProvider>
      </database>
    </databases>
  </sitecore>
</configuration>